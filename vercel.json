{
  "version": 2,
  "builds": [
    {
      "src": "COCO/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.13"
      },
      "buildCommand": "rm -rf staticfiles .vercel/cache && python manage.py collectstatic --noinput"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "COCO/wsgi.py"
    }
  ]
}
